ECHO OFF
ECHO SERVER %1
ECHO DATABASE %2

sqlcmd -s %1 -d %2 -o "IQ_DATABASE.CSV" -Q "SET NOCOUNT ON; SELECT NAME,OWNER_SID,COLLATION_NAME,USER_ACCESS_DESC,IS_READ_ONLY,STATE_DESC,RECOVERY_MODEL_DESC,CAST(CREATE_DATE AS VARCHAR(MAX)) AS CREATE_DATE FROM SYS.DATABASES"

IF %ERRORLEVEL% == 0 ECHO SCRIPT COMPLETED
